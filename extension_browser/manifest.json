{
    "manifest_version": 2,
    "name": "CrunchyTracker",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "http://www.crunchyroll.com/*"
        ],
        "js": ["content.js"]
      }
    ],
    "background":{
      "scripts": ["background.js"]
    },
    "permissions": ["tabs"],
    "browser_action": {
      "default_popup": "src/index.html",
      "default_title": "Track your anime consumption!"
    }
  }
  
