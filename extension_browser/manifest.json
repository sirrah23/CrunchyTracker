{
    "manifest_version": 2,
    "name": "CrunchyTracker",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "permissions": ["tabs"],
    "browser_action": {
      "default_popup": "src/index.html",
      "default_title": "Track your anime consumption!"
    }
  }
  